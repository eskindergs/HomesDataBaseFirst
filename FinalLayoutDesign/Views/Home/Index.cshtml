@{
    ViewBag.Title = "Home Page";
   
}


    <style>
        .jumbotron {
            margin: 80px 0;
            text-align: center;
            background-color: white; 
        }
        .jumbotron h1 {
            font-size: 7em;
            line-height: 1;
        }
